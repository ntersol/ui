{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "affected": {
    "defaultBase": "main"
  },
  "npmScope": "@ntersol",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "compodoc"
        ],
        "accessToken": "NGEwNjNhMzQtYmM3MS00NDg5LWJiYzQtZGJiZmExODE0ZmNmfHJlYWQtd3JpdGU=",
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "generators": {
    "@schematics/angular:component": {
      "style": "scss",
      "changeDetection": "OnPush",
      "export": true,
      "flat": true,
      "skipTests": true,
      "prefix": "nts"
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/workspace:library": {
      "publishable": true,
      "linter": "eslint",
      "strict": true,
      "prefix": "nts",
      "importPath": "@ntersol/<lib-name>",
      "skipBabelrc": true,
      "testEnvironment": "node",
      "unitTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "showcase"
}
