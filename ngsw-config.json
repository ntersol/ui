{
  "index": "/index.html",
  "routing": {
    "index": "/index.html",
    "/": {
      "match": "exact"
    },
    "/login": {
      "prefix": false
    }
  },
  
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/favicon.ico",
          "/apple-touch-icon.png",
          "/safari-pinned-tab.svg",
          "/*.css",
          "/*.js",
          "/*.js"
        ]
      }
    },
    {
      "name": "fonts",
      "installMode": "lazy",
      "resources": {
        "files": [
          "/*.woff",
          "/*.woff2",
          "/*.ttf",
          "/*.svg",
          "/*.eot"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "https://jsonplaceholder.typicode.com/**",
        "/users"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100000,
        "maxAge": "3d"
      }
    }
  ]
}
